{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "alaouy/youtube": "^2.2",
        "aporat/store-receipt-validator": "dev-master",
        "barryvdh/laravel-ide-helper": "^2.6",
        "bensampo/laravel-enum": "^1.28",
        "danhunsaker/bcmath": "^1.1",
        "doctrine/dbal": "^2.9",
        "elasticsearch/elasticsearch": "^7.4",
        "fideloper/proxy": "^4.0",
        "geoip2/geoip2": "^2.9",
        "google/apiclient": "^2.0",
        "google/cloud-vision": "^0.19.2",
        "gumlet/php-image-resize": "^1.9",
        "guzzlehttp/guzzle": "~6.0",
        "ixudra/curl": "^6.16",
        "james-heinrich/getid3": "^1.9",
        "lakshmaji/thumbnail": "^1.4",
        "laravel/framework": "^6.0",
        "laravel/passport": "^7.2",
        "laravel/tinker": "^1.0",
        "matthewbdaly/laravel-azure-storage": "^1.2",
        "mnabialek/laravel-sql-logger": "^2.2",
        "php-ffmpeg/php-ffmpeg": "^0.14.0",
        "predis/predis": "^1.1",
        "raiym/instagram-php-scraper": "^0.9.0",
        "spatie/crawler": "^4.6",
        "spatie/laravel-twitter-streaming-api": "^1.2",
        "thujohn/twitter": "^2.2"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "davidhsianturi/laravel-compass": "^0.3.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    }
}
